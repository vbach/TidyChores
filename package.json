{
  "name": "capstone-project-vbach",
  "version": "1.0.0",
  "engines": {
    "node": "10.15.0"
  },
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "reactjs": "npm start --prefix reactjs",
    "reactjs-install": "npm install --prefix reactjs",
    "db-update": "sequelize db:migrate; sequelize db:seed:all;",
    "dev": "concurrently \"npm run server\" \"npm run reactjs\"",
    "heroku-postbuild": "npm install --prefix reactjs && npm run build --prefix reactjs"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "morgan-debug": "^2.0.0",
    "nodemailer": "^6.4.2",
    "nodemailer-express-handlebars": "^3.1.0",
    "nodemon": "^2.0.2",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^7.14.0",
    "react-router": "^5.1.2",
    "react-router-prop-types": "^1.0.4",
    "sequelize": "^5.21.3",
    "sequelize-cli": "^5.5.1",
    "uuid": "^3.3.3",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "concurrently": "^4.1.2"
